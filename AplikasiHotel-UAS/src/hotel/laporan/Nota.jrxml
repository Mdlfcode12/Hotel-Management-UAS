	<parameter name="id_reservasi" class="java.lang.String">
		<defaultValueExpression><![CDATA[$P{id_reservasi}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    r.id_reservasi,
    t.nama_tamu,
    t.alamat,                
    k.tipe_kamar,
    k.harga AS harga_per_malam,
    r.tgl_checkin,
    r.tgl_checkout,
    DATEDIFF(r.tgl_checkout, r.tgl_checkin) AS lama_inap,
    r.total_bayar
FROM tabel_reservasi r
JOIN tabel_tamu t ON r.nik = t.nik
JOIN tabel_kamar k ON r.kode_kamar = k.kode_kamar
WHERE r.id_reservasi = $P{id_reservasi}]]>
	</queryString>
